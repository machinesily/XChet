<view><view class="status_bar"></view><top-bar class="topBar" vue-id="50cad900-1" bind:__l="__l" vue-slots="{{['center','right']}}"><view class="search-wrapper" slot="center"><input class="search" type="serach" placeholder="搜索用户/群" placeholder-style="color#aaa;" data-event-opts="{{[['input',[['search',['$event']]]]]}}" bindinput="__e"/><image class="search-img" src="../../static/images/search/search.png"></image></view><view class="cancel" slot="right" data-event-opts="{{[['tap',[['back',['$event']]]]]}}" bindtap="__e">取消</view></top-bar><view class="main"><view class="user"><view hidden="{{!(this.userarr.length>0)}}" class="title">用户</view><block wx:for="{{this.userarr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="userList"><navigator url="../userHome/userHome" hover-class="none3"><image src="{{item.image}}"></image></navigator><view class="names"><view class="name"><rich-text nodes="{{item.name}}"></rich-text></view><view class="email"><rich-text nodes="{{item.email}}"></rich-text></view></view><block wx:if="{{item.tip==1}}"><view class="button send">发消息</view></block><block wx:if="{{item.tip==0}}"><view class="button add">加好友</view></block></view></block></view></view></view>
<view><view class="status_bar"></view><top-bar class="topBar" vue-id="50cad900-1" bind:__l="__l" vue-slots="{{['center','right']}}"><view class="search-wrapper" slot="center"><input class="search" type="serach" placeholder="搜索用户/群" placeholder-style="color#aaa;" data-event-opts="{{[['input',[['__set_model',['','searchValue','$event',[]]],['Value',['$event']]]]]}}" value="{{searchValue}}" bindinput="__e"/><image class="search-img" src="../../static/images/search/search.png"></image></view><view class="cancel" slot="right" data-event-opts="{{[['tap',[['back',['$event']]]]]}}" bindtap="__e">取消</view></top-bar><view class="main"><view class="user"><view hidden="{{!(this.userarr.length>0)}}" class="title">用户</view><block wx:for="{{this.userarr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="userList"><navigator url="{{'../userHome/userHome?id='+item._id}}" hover-class="none3"><image src="{{item.imgurl}}"></image></navigator><view class="names"><view class="name"><rich-text nodes="{{item.name}}"></rich-text></view><view class="email"><rich-text nodes="{{item.mail}}"></rich-text></view></view><block wx:if="{{item.tip==1}}"><view data-event-opts="{{[['tap',[['toChatRoom',['$0'],[[['this.userarr','',index]]]]]]]}}" class="button send" bindtap="__e">发消息</view></block><block wx:if="{{item.tip==0}}"><view data-event-opts="{{[['tap',[['addFriendBtn',['$0'],[[['this.userarr','',index,'_id']]]]]]]}}" class="button add" bindtap="__e">加好友</view></block></view></block></view></view><view class="add-friend" style="{{'bottom:'+(-+addHeight+'px')+';'}}" animation="{{animationData}}"><view class="status_bar"></view><top-bar class="top-bar" vue-id="50cad900-2" bind:__l="__l" vue-slots="{{['left','center','right']}}"><view class="cancel" slot="left" data-event-opts="{{[['tap',[['addFriendPopUp',['$event']]]]]}}" bindtap="__e">取消</view><view class="title" slot="center">添加好友</view><view class="confirm" slot="right" data-event-opts="{{[['tap',[['addFriendConfirm',['$event']]]]]}}" bindtap="__e">确认</view></top-bar><view class="main"><view class="contnet-wrapper"><textarea data-event-opts="{{[['input',[['__set_model',['','message','$event',[]]]]]]}}" value="{{message}}" bindinput="__e"></textarea></view></view></view></view>